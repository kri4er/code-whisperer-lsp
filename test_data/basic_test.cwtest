
fn generate_random_string() -> String {
        let mut rng = rand::thread_rng();
    let random_string: String = rand::thread_rng()
        .sample_iter(&rand::distributions::Alphanumeric)
        .take(10)
        .map(char::from)
        .collect();
    return random_string;
}

